require "spec"
require "../src/*"

describe "<%-= to_capitalized(@json["exercise"].to_s) %>" do
<%- @json["cases"].as_a.each do |cases| %>
    <%= status()%> "<%-= cases["description"] %>" do
    palindrome = <%= to_capitalized(@json["exercise"].to_s) %>.new(<%= cases["input"]["min"] %>, <%= cases["input"]["max"] %>)
    <%- if cases["expected"]["error"]? -%>
    expect_raises(ArgumentError) do
        palindrome.<%= cases["property"].to_s.underscore %>()
    end
    <%- else -%>
    <%- if cases["expected"]["factors"].as_a.size == 2 -%>
    result = palindrome.<%= cases["property"].to_s.underscore %>
    result[0].should eq <%= cases["expected"]["value"] %>
    <%= "[#{to_s_digged(cases["expected"]["factors"])}, #{cases["expected"]["factors"].as_a.map{ |factor| to_s_digged(factor) }.reverse.to_s.gsub("\"", "")}]"%>.should contain result[1]
    <%- else -%>
    palindrome.<%= cases["property"].to_s.underscore %>.should eq({<%= cases["expected"]["value"].as_i? ? cases["expected"]["value"] :  "nil" %> , <%= cases["expected"]["factors"].as_a.empty? ? "#{ to_s_digged(cases["expected"]["factors"])} of Int32": to_s_digged(cases["expected"]["factors"]) %>})
    <%- end -%>
    <%- end %>
    end
<% end %>
end
