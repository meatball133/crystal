require "spec"
require "../src/*"

describe "<%-= to_capitalized(@json["exercise"].to_s) %>" do
<%- @json["cases"].as_a.each do |cases| %>
    <%= status()%> "<%-= cases["description"] %>" do
        sentence = <%= cases["input"]["sentence"].to_s.inspect %>
        expected = <%= to_s_deep(cases["expected"]) %>
        <%= to_capitalized(@json["exercise"].to_s) %>.<%= cases["property"].to_s.underscore %>(sentence).should eq(expected)
    end
<% end %>
end
